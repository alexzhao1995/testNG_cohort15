<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">


	<test thread-count="5" name="Parallel Tests" parallel="methods">
		<!-- parallel="methods" opens all tests at once-->
		<!-- parallel="classes" opens multiple classes at once make sure class is listed below-->
		<!-- thread-count controls how many browsers can open-->
		<classes>
			<class name="testNG_day03.ParallelTest" />
		</classes>
	</test> <!--Test -->


</suite> <!--Suite -->
